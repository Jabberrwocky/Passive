    <projectUrl>https://github.com/Talljoe/Passive</projectUrl>
  </metadata>
  <files>
    <file src="build\Passive.Async\*" target="lib\net40" />
  </files>
  <dependencies>
    <dependency id="Rx-Core" version="1.0.0.0" />
    <dependency id="Rx-Interactive" version="1.0.0.0" />
    <dependency id="Rx-AsyncLinq" version="1.0.0.0" />
  </dependencies>
</package>